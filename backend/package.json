{
  "name": "neurovox-backend",
  "version": "1.0.0",
  "description": "Local Whisper backend server for NeuroVox plugin",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node --max-old-space-size=8192 dist/server.js",
    "dev": "node --max-old-space-size=8192 -r ts-node/register src/server.ts"
  },
  "keywords": [
    "whisper",
    "transcription",
    "speech-to-text"
  ],
  "author": "Synaptic Labs",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@huggingface/transformers": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.23.2",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
